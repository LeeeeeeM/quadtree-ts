{
  "name": "@timohausmann/quadtree-ts",
  "version": "2.2.2",
  "description": "Quadtree Typescript Implementation",
  "author": "Timo Hausmann",
  "license": "MIT",
  "main": "dist/quadtree.cjs.js",
  "module": "dist/quadtree.esm.js",
  "browser": "dist/quadtree.umd.full.js",
  "types": "types/index.esm.d.ts",
  "scripts": {
    "build": "npm run rollup && npm run docs && npm run dts",
    "rollup": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --fix --ext .ts src/",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "test": "jest --verbose",
    "docs": "typedoc --out docs/documentation --readme none src/index.esm.ts",
    "dts": "tsc src/index.esm.ts --declaration --emitDeclarationOnly --outDir ./types/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timohausmann/quadtree-ts.git"
  },
  "bugs": {
    "url": "https://github.com/timohausmann/quadtree-ts/issues"
  },
  "keywords": [
    "quadtree"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "types/",
    "README.md",
    "LICENSE.md"
  ],
  "directories": {
    "example": "docs"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@tsconfig/recommended": "^1.0.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tsdoc": "^0.4.0",
    "expect-type": "^1.2.2",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rollup": "^4.52.5",
    "rollup-plugin-banner2": "^1.3.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "exports": {
    "types": "./types/index.esm.d.ts",
    "node": "./dist/quadtree.cjs.js",
    "default": "./dist/quadtree.esm.js"
  }
}
